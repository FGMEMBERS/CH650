<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>interior.ac</path>

 

  <animation>
    <type>noshadow</type>
    <object-name>windowFint</object-name>
    <object-name>windowRint</object-name>
    <object-name>doorRglassint</object-name>
    <object-name>doorLglassint</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>doorRglassint</object-name>
    <object-name>doorLglassint</object-name>
    <object-name>windowFint</object-name>
    <object-name>windowRint</object-name>
  </animation>

<!--Doors-->
  <animation>
    <type>rotate</type>
    <object-name>doorLglassint</object-name>
    <property>instrumentation/doors/crew/position-norm</property>
 <interpolation>
      <entry><ind> 0 </ind><dep>  0 </dep></entry>
      <entry><ind> 0.2 </ind><dep>  0 </dep></entry>
      <entry><ind> 1 </ind><dep> -70 </dep></entry>
    </interpolation>
    <center>
      <x-m> 0.55109 </x-m>
      <y-m> -0.51726 </y-m>
      <z-m>0.49834 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z>0 </z>
    </axis>
  </animation>


  <animation>
    <type>rotate</type>
    <object-name>doorRglassint</object-name>
    <property>instrumentation/doors/passenger/position-norm</property>
 <interpolation>
      <entry><ind> 0 </ind><dep>  0 </dep></entry>
      <entry><ind> 0.2 </ind><dep>  0 </dep></entry>
      <entry><ind> 1 </ind><dep> 70 </dep></entry>
    </interpolation>
    <center>
      <x-m> 0.55109 </x-m>
      <y-m> 0.51726 </y-m>
      <z-m>0.49834 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z>0 </z>
    </axis>
  </animation>

  <!-- Panel -->
  <model>
    <path>Panel/panel.xml</path>
  </model>
<!--Pedal animation-->

  <animation>
    <type>rotate</type>
    <object-name>Circle.003</object-name>
    <object-name>Mesh.221</object-name>
    <object-name>Mesh.215</object-name>
    <property>controls/flight/rudder</property>
 <interpolation>
      <entry><ind> -1 </ind><dep>  -30 </dep></entry>
      <entry><ind> 0 </ind><dep>  0 </dep></entry>
      <entry><ind> 1 </ind><dep> 30 </dep></entry>
    </interpolation>
    <factor> 15 </factor>
    <center>
      <x-m>  -1.06871 </x-m>
      <z-m>-0.43718</z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>Mesh.221</object-name>
    <object-name>Mesh.215</object-name>
    <property>controls/flight/rudder</property>
 <interpolation>
      <entry><ind> -1 </ind><dep>  30 </dep></entry>
      <entry><ind> 0 </ind><dep>  0 </dep></entry>
      <entry><ind> 1 </ind><dep> -30 </dep></entry>
    </interpolation>
    <factor> 15 </factor>
    <center>
      <x-m> -1.11185 </x-m>
      <z-m>-0.31865</z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>Circle.004</object-name>
    <object-name>Mesh.011</object-name>
    <object-name>Mesh.008</object-name>
    <property>controls/flight/rudder</property>
 <interpolation>
      <entry><ind> -1 </ind><dep>  30 </dep></entry>
      <entry><ind> 0 </ind><dep>  0 </dep></entry>
      <entry><ind> 1 </ind><dep> -30 </dep></entry>
    </interpolation>
    <factor> 15 </factor>
    <center>
      <x-m> -1.0366</x-m>
      <z-m> -0.43797</z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>Mesh.011</object-name>
    <object-name>Mesh.008</object-name>
    <property>controls/flight/rudder</property>
 <interpolation>
      <entry><ind> -1 </ind><dep>  -30 </dep></entry>
      <entry><ind> 0 </ind><dep>  0 </dep></entry>
      <entry><ind> 1 </ind><dep> 30 </dep></entry>
    </interpolation>
    <factor> 15 </factor>
    <center>
      <x-m> -1.08295</x-m>
      <z-m>-0.30794</z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
<!--Yoke Animation-->
<animation>
<name>yokeL</name>
    <object-name>Stick - left</object-name>
    <object-name>Stick - left.001</object-name>
</animation>
  <animation>
    <type>rotate</type>
    <object-name>yokeL</object-name>
    <property>controls/flight/elevator</property>
    <factor> 15 </factor>
    <center>
      <x-m>  -0.60377 </x-m>
      <y-m>-0.28222</y-m>
      <z-m> -0.4535</z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> -1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>yokeL</object-name>
    <property>controls/flight/aileron</property>
